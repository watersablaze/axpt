{
  "version": 2,
  "buildCommand": "bash app/scripts/verify-env.sh && pnpm install && pnpm build",
  "outputDirectory": ".next",
  "framework": "nextjs",
  "env": {
    "NODE_ENV": "production"
  },
  "routes": [
    {
      "src": "/",
      "dest": "/landing"
    },
    {
      "src": "/vault/(.*)",
      "dest": "/vault/[doc]"
    },
    {
      "src": "/onboard/investor",
      "dest": "/onboard/investor"
    },
    {
      "src": "/onboard/investor/dashboard",
      "dest": "/onboard/investor/dashboard"
    },
    {
      "src": "/docs/(.*)",
      "dest": "/docs/[doc]"
    }
  ]
}