{
  "rewrites": [
    {
      "source": "/",
      "destination": "/onboard/investor"
    }
  ],
  "build": {
    "env": {
      "NODE_ENV": "production"
    }
  },
  "functions": {
    "app/api/partner/**/route.ts": {
      "memory": 256,
      "maxDuration": 10
    }
  },
  "ignoreCommand": "echo Ignoring non-investor pages during deployment.",
  "outputDirectory": ".next"
}