{
    "cleanUrls": true,
    "trailingSlash": false,
    "rewrites": [
      {
        "source": "/partner/whitepaper",
        "destination": "/partner/whitepaper"
      }
    ],
    "headers": [
      {
        "source": "/whitepaper/:file*",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public, max-age=3600, must-revalidate"
          },
          {
            "key": "Access-Control-Allow-Origin",
            "value": "*"
          }
        ]
      }
    ],
    "static": {
      "includeFiles": [
        "public/whitepaper/AXPT-Whitepaper.pdf"
      ]
    }
  }